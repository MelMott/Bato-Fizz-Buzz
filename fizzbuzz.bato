iprint "kumusta"

bilang FizzBuzz

  # ang = 'def' in bato
  ang fizzbuzz(numero)

    resulta = ''

    kapag numero % 3 == 0
      resulta = resulta + 'Fizz'
    wakas
    kapag numero % 5 == 0
      resulta = resulta + 'Buzz'
    kung_hindi
      resulta = numero
    wakas

    iprint resulta
    resulta
  wakas


wakas

fizzbuzz = FizzBuzz.gumawa
fizzbuzz.fizzbuzz(1)
fizzbuzz.fizzbuzz(3)
fizzbuzz.fizzbuzz(5)
fizzbuzz.fizzbuzz(15)
